{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "search-app-store",
  "title": "Search App Store",
  "description": "Search the App Store right from Raycast!",
  "icon": "app-store.png",
  "author": "jabir",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-app-store",
      "title": "Search App Store",
      "description": "Template for running a script without any view",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": true
        }
        
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.1",
    "@raycast/utils": "^1.9.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
